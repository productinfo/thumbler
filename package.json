{
  "name": "thumbler",
  "version": "0.3.4",
  "private": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prestart": "yarn build",
    "start": "node ./build/app",
    "start-dev": "node bin/www",
    "format": "prettier-eslint \"**/*.js\" --write",
    "dev": "webpack -w",
    "build": "webpack"
  },
  "eslintConfig": {
    "extends": "toggl"
  },
  "eslintIgnore": [
    "/public/**"
  ],
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "workspaces": [
    "toggl/*"
  ],
  "dependencies": {
    "accepts": "^1.3.0",
    "body-parser": "~1.18.2",
    "coffeescript": "~2.1.1",
    "compression": "~1.7.1",
    "consolidate": "~0.15.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.7.1",
    "debug": "~3.1.0",
    "eco": "~1.1.0-rc-3",
    "express": "~4.16.2",
    "express-paginate": "0.3.0",
    "lodash": "^4.3.0",
    "moment": "^2.10.6",
    "mongoose": "~4.13.9",
    "mongoose-paginate": "5.0.3",
    "morgan": "~1.9.0",
    "process": "~0.11.10",
    "q": "~1.5.1",
    "serve-favicon": "~2.4.5",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "chai": "~4.1.2",
    "chai-as-promised": "~7.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.18.2",
    "gulp": "~3.9.1",
    "gulp-bump": "~3.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-livereload": "~3.8.1",
    "gulp-print": "~2.0.1",
    "gulp-shell": "~0.6.5",
    "gulp-spawn-mocha": "~4.0.1",
    "gulp-supervisor": "~0.1.0",
    "gulp-tap": "~1.0.1",
    "gulp-util": "~3.0.8",
    "husky": "^0.14.3",
    "istanbul": "^0.4.3",
    "lint-staged": "^7.0.0",
    "methods": "~1.1.0",
    "mocha": "~4.1.0",
    "prettier": "1.11.1",
    "prettier-eslint-cli": "^4.7.1",
    "superagent": "~3.8.2",
    "supertest": "~3.0.0",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13"
  }
}
